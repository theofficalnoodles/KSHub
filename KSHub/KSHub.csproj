<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>WinExe</OutputType>
    <TargetFramework>net6.0-windows</TargetFramework>
    <UseWPF>true</UseWPF>
    <Nullable>enable</Nullable>
    <AssemblyName>KSHub</AssemblyName>
    <RootNamespace>KSHub</RootNamespace>
    <ApplicationIcon>..\assets\kshub.ico</ApplicationIcon>
  </PropertyGroup>

  <ItemGroup>
    <!-- Use a CefSharp version compatible with your target (update if necessary) -->
    <PackageReference Include="CefSharp.Wpf" Version="109.1.390" />
    <PackageReference Include="CefSharp.Common" Version="109.1.390" />
  </ItemGroup>

</Project>
